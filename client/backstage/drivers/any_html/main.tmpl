<tmpl id="anyHtml">
	<div class="any_html">
		<div class="driver_warning">
			`<b>DANGEROUS</b> Do not edit unless you know what the contents mean to the website.`
		</div>
		<div class="driver_abstract">
			<textarea name="abstract" placeholder="`Abstract`">{{abstract}}</textarea>
		</div>
		<div class="driver_content">
			<textarea name="content" placeholder="`Content`">{{content}}</textarea>
		</div>
	</div>
</tmpl>